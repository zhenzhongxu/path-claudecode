{
  "agentCanModify": [
    ".claude/path-kernel/event-log.jsonl",
    ".claude/path-kernel/state.json",
    ".claude/rules/skill/*",
    ".claude/rules/valence/*",
    ".claude/rules/world/*",
    "CLAUDE.md"
  ],
  "agentCannotModify": [
    ".claude/path-kernel/config.json",
